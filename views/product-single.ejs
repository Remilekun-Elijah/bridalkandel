                                document.querySelectorAll(".details-brand").forEach(f => f.innerText = e.uploadedBy);
                                document.querySelectorAll(".details-price .price").forEach(f => f.innerText = ` ₦${e.price}`);
                                document.querySelectorAll(".discount").forEach(f => f.innerText = `₦${e.discount}`);
                                document.querySelectorAll('.details-desc').forEach(f => f.innerText = e.description);
                                document.querySelectorAll('.category').forEach(f => f.innerText = e.category);
                                // console.log(document.querySelector(".discount"));
                                let f = '';
                                e.size.forEach(p => {
                                    // f = document.createElement('li');
                                    f += `<li><a class="size" href="#">${p}</a></li>`;
                                })
                                document.querySelector('.details-size-list').innerHTML = f;
                            })
                        </script>
